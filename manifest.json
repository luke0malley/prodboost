{
  "manifest_version": 3,
  "name": "My_ext",
  "version": "0.1.0",
  "description": "My Chrome Extension",
  "action": {
    "default_title": "My_ext",
    "default_popup": "popup.html"
  },
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_idle",
      "js": ["contentScript.js"]
    }
  ]
}
